{
    "manifest_version": 3,
    "name": "OwlGuide",
    "short_name": "OwlGuide",
    "description": "Assist with Textbook Management",
    "version": "2.0.5",
    "icons": [
        {
            "src": "/owl.ico",
            "sizes": "256x256",
            "type": "image/ico"
        }
    ],
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "*://*/*",
        "http://*/*",
        "https://*/*"
    ]
}